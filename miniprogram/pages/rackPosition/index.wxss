page{
    background-color: #f5f5f5;
}

.tree-select{position:relative;font-size:28rpx;-webkit-user-select:none;user-select:none;}
.tree-select__nav{position:absolute;top:0;bottom:0;left:0;width:35%;min-width:120px;background-color:#fafafa}
.tree-select__content{width:65%;margin-left:35%;background-color:#fff;box-sizing:border-box}
.tree-select__nav .items,.tree-select__content .items{
    position: relative;
    padding: 0 20rpx;
    text-align: center;
    color: #333;
    font-size: 32rpx;
    height: 100rpx;
    line-height: 100rpx;
}
.tree-select__content .items{
    padding-left: 90rpx;
    text-align: left;
}
.tree-select__content .items text{
    display: inline-block;
    width: 120rpx;
}
.tree-select__content .items.main-active-class{
    color: #074195;
    background-color: #fff;
}
.tree-select__nav .items.main-active-class{
    color: #074195;
    background-color: #fff;
}
.tree-select__nav .items.main-active-class .active-line{
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
    width: 10rpx;
    background-color: #074195;
}
.active-item{
    font-weight:bold;
    color: #074195;
}
.van-tag{
    font-size: 24rpx !important;
    margin-right: 15rpx;
    font-weight: 900;
    min-width: 112rpx;
    text-align: center;
}
.tree-select button{
    padding: 0;
    width: 41.6rpx;
    height: 41.6rpx;
    text-align: center;
    line-height: 37rpx;
}
.home-search{
    position: relative;
    width: 100%;
}
.home-search .scanCode-image{
    position: absolute;
    display: block;
    right: 30px;
    top: 17.5px;
    width: 20px;
    height: 20px;
    z-index: 50;
}
.home-search input{
    width: 80%;
    padding-right: 50px !important;
    font-size: 28rpx;
}
.rack-position{
    width: 100%;
    margin-top: 20rpx;
}